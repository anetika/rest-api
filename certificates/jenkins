pipeline {
    agent any
    options {
        buildDiscarder(logRotator(numToKeepStr: '5'))
        }
        stages {
            stage('Scan') {
                steps {
                    dir('./certificates') {
                        sh 'pwd'
                    }
                    withSonarQubeEnv(installationName: 'sonarqube') {
                       sh "./gradlew sonarqube"
                    }
                }
            }
        }
    }